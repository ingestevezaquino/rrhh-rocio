@model IEnumerable<ProyectoRRHH.Models.departamento>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <thead>
        <tr>
            <th>
                <h2>Departamentos</h2>
                <p>
                    <a asp-action="Create" class="btn btn-success">Agregar</a>
                </p>
            </th>
            <th>
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.departamento1)
            </td>
            <td>
                <a asp-action="Edit" class="btn btn-warning" asp-route-id="@item.id">Editar</a> |
                    <!--<a asp-action="Details" class="btn btn-primary" asp-route-id="@item.id">Detalles</a>-->
                <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.id">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
